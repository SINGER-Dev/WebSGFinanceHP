
@{
    ViewData["Title"] = " ระบบ ยกเลิกใบคำขอ  SG Finance+ ";
}


@using (Html.BeginForm("SearchGetApplicationHistory", "Home", FormMethod.Post, new { id = "searchFormApplicationHistory" }))
{
    <div class="mb-3 row">
        <label for="ApplicationCode" class="col-sm-2 col-form-label">เลขที่ใบคำขอ</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="ApplicationCode" name="ApplicationCode" placeholder="เลขที่ใบคำขอ">
        </div>

        <label for="AccountNo" class="col-sm-2 col-form-label">บัญชีเลขที่</label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="AccountNo" name="AccountNo" placeholder="บัญชีเลขที่">
        </div>
    </div>

    <div class="mb-3 row">
        <label for="startdate" class="col-sm-2 col-form-label">วันที่เริ่ม</label>
        <div class="col-sm-4">
            <input type="text" class="date form-control" id="startdate" name="startdate" placeholder="วันที่เริ่ม">
        </div>

        <label for="enddate" class="col-sm-2 col-form-label">วันที่สิ้นสุด</label>
        <div class="col-sm-4">
            <input type="text" class="date form-control" id="enddate" name="enddate" placeholder="วันที่สิ้นสุด">
        </div>
    </div>

    <div class="mb-3 row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary mb-3">ค้นหา</button>
        </div>
    </div>

}

<div class="loaddong">
    <div class="spinner-border" role="status">
        <span class="visually-hidden">Loading...</span>
    </div>
</div>
<div id="searchResults"></div>